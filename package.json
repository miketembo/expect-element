{
  "name": "expect-element",
  "version": "1.1.0",
  "description": "Write better assertions for DOM nodes",
  "author": "Michael Jackson",
  "license": "MIT",
  "main": "lib",
  "browser": "umd/expect-element.min.js",
  "files": [
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "node ./scripts/build.js",
    "build-cjs": "babel ./modules -d lib --ignore '__tests__'",
    "build-umd": "webpack modules/index.js umd/expect-element.js",
    "build-min": "webpack -p modules/index.js umd/expect-element.min.js",
    "lint": "eslint modules",
    "test": "npm run lint && karma start",
    "release": "node ./scripts/release.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mjackson/expect-element.git"
  },
  "homepage": "https://github.com/mjackson/expect-element",
  "peerDependencies": {
    "expect": "^1.15.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "expect": "^1.12.2",
    "gzip-size": "^3.0.0",
    "karma": "^0.13.3",
    "karma-browserstack-launcher": "^0.1.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.0.1",
    "pretty-bytes": "^2.0.1",
    "readline-sync": "^1.4.1",
    "webpack": "^1.4.13"
  },
  "keywords": [
    "expect",
    "assert",
    "test",
    "spec",
    "dom"
  ]
}
