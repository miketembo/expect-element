{
  "name": "expect-dom",
  "version": "1.0.0",
  "description": "Expect-style assertions for DOM nodes",
  "main": "index.js",
  "scripts": {
    "build": "babel ./modules -d lib --stage 0 --loose all --ignore '__tests__'",
    "build-umd": "NODE_ENV=production webpack modules/index.js umd/expect-dom.js",
    "build-min": "NODE_ENV=production webpack -p modules/index.js umd/expect-dom.min.js",
    "lint": "eslint modules",
    "test": "npm run lint && karma start",
    "postinstall": "node ./npm-scripts/postinstall.js"
  },
  "author": "Michael Jackson",
  "repository": {
    "type": "git",
    "url": "git://github.com/mjackson/expect-dom.git"
  },
  "bugs": {
    "url": "https://github.com/mjackson/expect-dom/issues"
  },
  "homepage": "https://github.com/mjackson/expect-dom",
  "license": "MIT",
  "peerDependencies": {
    "expect": "^1.12.2"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.0.0",
    "eslint": "^1.3.1",
    "expect": "^1.12.2",
    "gzip-size": "^3.0.0",
    "karma": "^0.13.3",
    "karma-browserstack-launcher": "^0.1.6",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.0.1",
    "pretty-bytes": "^2.0.1",
    "webpack": "^1.4.13"
  },
  "keywords": [
    "expect",
    "assert",
    "test",
    "spec",
    "dom"
  ]
}
